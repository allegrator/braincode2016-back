{
    "rules": {
        "indent": [
            0,
            2
        ],
        "quotes": [
            1,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "no-console": [1],
        "comma-dangle": [0],
        "no-use-before-define": [2],
        "array-bracket-spacing": [2, "never"],
        "eol-last": [1],
        "no-undef": [1],
        "arrow-body-style": [1, "as-needed"],
        "object-shorthand": [1, "methods"],
        "prefer-template": [1],
        "no-extra-parens": [1],
        "no-dupe-class-members": [2],
        "object-curly-spacing": [2, "always", {"objectsInObjects": false, "arraysInObjects": false}],
        "space-infix-ops": [2],
        "eqeqeq": [2],
        "no-fallthrough": [2],
        "no-invalid-this": [2],
        "no-extra-bind": [2],
        "no-extend-native": [2],
        "accessor-pairs": [2],
        "complexity": [1, 15],
        "no-unreachable": [1],
        "use-isnan": [1],
        "no-empty": [1],
        "no-func-assign": [2],
        "no-sparse-arrays": [2],
        "no-irregular-whitespace": [2],
        "no-cond-assign": [2],
        "no-case-declarations": [2],
        "no-floating-decimal": [2],
        "no-self-compare": [2],
        "no-loop-func": [2],
        "no-with": [2],
        "wrap-iife": [2, "inside"],
        "yoda": [2, "never"],
        "no-useless-concat": [2],
        "no-new-wrappers": [2],
        "no-eq-null": [2],
        "no-empty-pattern": [2],
        "no-proto": [2],
        "radix": [2],
        "dot-location": [2, "property"],
        "consistent-return": [2],
        "space-before-function-paren": [2, "never"],
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "keyword-spacing": [2]
    },
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended"
}
